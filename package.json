  },
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:visual": "playwright test --project=visual",
    "test:e2e:visual:update": "playwright test --project=visual --update-snapshots",
    "test:e2e:visual:ci": "playwright test --project=visual --reporter=list",
    "test:e2e:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:e2e:all": "playwright test --project=chromium --project=firefox --project=webkit --project=visual",
    "test:visual:generate": "playwright test --project=visual && echo 'Baseline screenshots generated in tests/screenshots/baseline/'",
    "deploy": "bun run build && wrangler deploy"
  },
